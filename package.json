{
  "name": "dets",
  "version": "0.5.4",
  "description": "A TypeScript declaration file bundler.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "ts-node example/play.ts",
    "build": "npm run bundle && npm run transpile && cp lib/cli.js dist/ && npm run declare",
    "bundle": "ncc build src/index.ts --external typescript --out dist",
    "declare": "node dist/cli.js --files src/index.ts --imports typescript --types src/index.ts --out dist/index.d.ts",
    "transpile": "tsc",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "declaration",
    "bundler",
    "tds",
    "file",
    "generate",
    "types"
  ],
  "author": "Florian Rappl <mail@florian-rappl.de>",
  "license": "MIT",
  "engines": {
    "node": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianRappl/dets.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianRappl/dets/issues"
  },
  "bin": {
    "dets": "./lib/cli.js"
  },
  "dependencies": {
    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "typescript": "3.x"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.23",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "@types/yargs": "^15.0.4",
    "@zeit/ncc": "^0.21.1",
    "deox": "^3.2.1",
    "glob": "^7.1.6",
    "jest": "^25.1.0",
    "piral-core": "^0.10.9",
    "redux-saga": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.11"
  }
}
